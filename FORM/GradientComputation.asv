function Y = GradientComputation(X)
vars_size = length(X)
Gradient = zeros(vars_size,1)
for var_iterator = 1:vars_size
    varPerturbed = X(var_iterator);
    stepSize = varPerturbed;
    x = X;
    x(var_iterator) = varPerturbed + stepSize
    G_forward = objFun(x)
    x = X
    x = X;
    x(var_iterator) = varPerturbed - stepSize;
    G_backward = objFun(x);
    Gradient(vars_size) = (G_forward - G_backward)/(2*stepSize);
end
end
